{
  "service": {
    "name": "<%= @traefik_project %>-traefik",
    "port": 8082,
    "tags": [
      "<%= @traefik_project %>",
      "traefik.tags=<%= @tag_name %>",
      "traefik.frontend.rule=PathPrefixStrip:/traefik"
    ],
    "check": {
       "http": "http://localhost:8082/health",
       "interval": "10s",
       "timeout": "1s"
    }
  }
}
